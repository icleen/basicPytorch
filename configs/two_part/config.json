{
  "task": "two_part",
  "type": "two_part",

  "batch_size": 2,
  "vbatch_size": 8,
  "model_def": {
    "yolo": "configs/two_part/model_yolo.cfg",
    "regress": "configs/two_part/model_regress.cfg"
  },

  "data_config": {
    "type": "csv",
    "classes": 2,
    "train": "configs/twoobj/train.csv",
    "valid": "configs/twoobj/valid.csv",
    "names": "configs/twoobj/classes.names"
  },
  "pretrained_weights": "weights/darknet53.conv.74",

  "learning_rate": 0.0001,
  "n_cpu": 1,
  "input_dim": 3,
  "img_size": {
    "yolo": 416,
    "regress": 216
  },
  "checkpoint_interval": 1,
  "evaluation_interval": 1,
  "compute_map": false,
  "multiscale_training": true,
  "gradient_accumulations": 2,
  "epochs": 20,

  "iou_thres": 0.5,
  "conf_thres": 0.8,
  "nms_thres": 0.5,

  "checkpoint_path": "checkpoints/",
  "log_path": "two_part_logs",
  "val_metrics": "two_part_metrics.txt"

}
